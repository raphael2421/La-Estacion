   <style>
      @charset "UTF-8";

      :root {
         --Background: #fff;
         --FontLight: #c5bba9;
         --FontDark: #322a26;
         --Primary: #c5bba9;
         --Secondary: #d7340f
      }

      /*! normalize.css v8.0.1 | MIT License | github.com/necolas/normalize.css */
      html {
         line-height: 1.15;
         -webkit-text-size-adjust: 100%
      }

      body {
         margin: 0
      }

      main {
         display: block
      }

      h1 {
         font-size: 2em;
         margin: .67em 0
      }

      hr {
         box-sizing: content-box;
         height: 0;
         overflow: visible
      }

      pre {
         font-family: monospace, monospace;
         font-size: 1em
      }

      a {
         background-color: transparent
      }

      abbr[title] {
         border-bottom: none;
         text-decoration: underline;
         text-decoration: underline dotted
      }

      b,
      strong {
         font-weight: bolder
      }

      code,
      kbd,
      samp {
         font-family: monospace, monospace;
         font-size: 1em
      }

      small {
         font-size: 80%
      }

      sub,
      sup {
         font-size: 75%;
         line-height: 0;
         position: relative;
         vertical-align: baseline
      }

      sub {
         bottom: -.25em
      }

      sup {
         top: -.5em
      }

      img {
         border-style: none
      }

      button,
      input,
      optgroup,
      select,
      textarea {
         font-family: inherit;
         font-size: 100%;
         line-height: 1.15;
         margin: 0
      }

      button,
      input {
         overflow: visible
      }

      button,
      select {
         text-transform: none
      }

      [type=button],
      [type=reset],
      [type=submit],
      button {
         -webkit-appearance: button
      }

      [type=button]::-moz-focus-inner,
      [type=reset]::-moz-focus-inner,
      [type=submit]::-moz-focus-inner,
      button::-moz-focus-inner {
         border-style: none;
         padding: 0
      }

      [type=button]:-moz-focusring,
      [type=reset]:-moz-focusring,
      [type=submit]:-moz-focusring,
      button:-moz-focusring {
         outline: 1px dotted ButtonText
      }

      fieldset {
         padding: .35em .75em .625em
      }

      legend {
         box-sizing: border-box;
         color: inherit;
         display: table;
         max-width: 100%;
         padding: 0;
         white-space: normal
      }

      progress {
         vertical-align: baseline
      }

      textarea {
         overflow: auto
      }

      [type=checkbox],
      [type=radio] {
         box-sizing: border-box;
         padding: 0
      }

      [type=number]::-webkit-inner-spin-button,
      [type=number]::-webkit-outer-spin-button {
         height: auto
      }

      [type=search] {
         -webkit-appearance: textfield;
         outline-offset: -2px
      }

      [type=search]::-webkit-search-decoration {
         -webkit-appearance: none
      }

      ::-webkit-file-upload-button {
         -webkit-appearance: button;
         font: inherit
      }

      details {
         display: block
      }

      summary {
         display: list-item
      }

      template {
         display: none
      }

      [hidden] {
         display: none
      }

      body {
         font-family: 'Encode Sans', sans-serif
      }

      .container {
         background-color: #fff;
         width: 100%;
         position: relative;
         overflow: hidden;
         padding-bottom: 38px
      }

      .hidden {
         visibility: hidden !important
      }

      .display_none {
         display: none !important
      }

      html {
         scroll-behavior: smooth
      }

      header {
         background-color: var(--Primary);
         width: auto;
         height: 44px;
         padding: 0 26px;
         display: grid;
         grid-template-columns: 35px auto 35px;
         align-items: flex-end;
         position: sticky;
         top: 0;
         z-index: 51;
         box-shadow: 1px 1px 2px rgba(0, 0, 0, .1)
      }

      .header_logo {
         height: 100%;
         display: grid;
         align-content: center
      }

      .header_logo>img {
         height: 35px;
         margin: 5px auto
      }

      .header_icons {
         height: 100%;
         position: relative;
         display: flex;
         flex-direction: row-reverse;
         margin-right: 26px
      }

      .header_icons>img {
         right: 0;
         padding-left: 16px;
         width: 26px
      }

      .tel_icon {
         height: 22px;
         width: 19px;
         margin-top: 12px;
         margin-right: 16px
      }

      .whats_icon {
         width: 26px;
         margin-top: 3px
      }

      .header_menu {
         height: 100%;
         display: grid;
         align-content: center
      }

      .header_menu>img {
         height: 21px;
         width: 24px;
         margin: 12px auto
      }

      footer {
         position: relative
      }

      footer {
         height: 38px;
         position: fixed;
         width: 100%;
         display: grid;
         grid-template-rows: 17px 21px;
         bottom: 0;
         justify-items: center;
         background-color: unset
      }

      footer>.acento {
         align-self: end
      }

      .acento {
         display: block;
         width: 52px;
         height: 6px;
         background-color: var(--Secondary);
         margin: 0 auto
      }

      .la-estacion-logo {
         height: 35px;
         align-self: flex-end;
         background-color: var(--Primary);
         padding: 2px
      }

      .close_menu_icon,
      .menu_icon,
      .tel_icon,
      .whats_icon {
         cursor: pointer
      }

      .menu_top {
         display: none;
         background-color: var(--Primary);
         position: fixed;
         top: 44px;
         z-index: 50;
         width: 100%;
         height: 100vh;
         justify-items: center
      }

      .menu_top>ul {
         padding: 0
      }

      .menu_top>ul>li {
         font-family: 'Encode Sans', sans-serif;
         object-fit: contain;
         list-style: none;
         text-transform: uppercase;
         font-size: 21px;
         font-weight: 100;
         font-stretch: normal;
         font-style: normal;
         line-height: 2.29;
         letter-spacing: 6.3px;
         text-align: center
      }

      .menu_link {
         color: var(--FontDark);
         text-decoration: none
      }

      .menu_link:hover {
         color: var(--Secondary)
      }

      .close_menu>img {
         width: 33px;
         height: 14px;
         margin: 0 auto
      }

      .menu_top_open {
         display: grid;
         animation: openMenu 350ms;
         overflow: hidden
      }

      @keyframes openMenu {
         0% {
            top: -100px;
            opacity: 0
         }

         100% {
            top: 44px;
            opacity: 1
         }
      }

      .menu_top_close {
         display: grid;
         animation: closeMenu 350ms forwards
      }

      @keyframes closeMenu {
         0% {
            top: 44px;
            opacity: 1
         }

         100% {
            top: -100px;
            opacity: .1
         }
      }

      .menu_desk {
         display: none
      }

      @media screen and (min-width:992px) {
         .menu_top {
            display: none
         }

         header {
            background-color: var(--Primary);
            width: auto;
            height: 60px;
            padding: 0 5%;
            display: grid;
            grid-template-columns: 94px auto;
            align-items: flex-end;
            position: sticky;
            top: 0;
            z-index: 51;
            box-shadow: 1px 1px 2px rgba(0, 0, 0, .1);
            position: sticky
         }

         .header_logo>img {
            height: 60px;
            background-color: var(--Primary);
            padding-top: 40px
         }

         .whats_icon {
            width: 36px;
            margin-top: 12px;
            margin-left: 30px
         }

         .header_menu>img {
            height: 26px;
            width: 32px;
            padding-bottom: 30px
         }

         .tel_icon {
            display: none
         }

         .menu_desk {
            display: block
         }

         .menu_desk>ul>li {
            font-family: 'Encode Sans', sans-serif;
            object-fit: contain;
            list-style: none;
            text-transform: uppercase;
            font-size: 16px;
            font-weight: inherit;
            font-stretch: normal;
            font-style: normal;
            line-height: 2;
            text-align: center;
            float: left;
            margin-right: 18px
         }

         .header_menu {
            display: none
         }
      }

      .link_active {
         color: var(--Secondary)
      }

      .touch_anim {
         width: 70px;
         height: 70px;
         position: absolute;
         bottom: 180px;
         right: 40px;
         z-index: 100;
         transition: .5s;
         animation: finger_fadein .8s forwards;
         animation-delay: 1.2s;
         opacity: 0;
         z-index: 100
      }

      .finger_fadeout {
         animation: finger_fadeout .5s forwards
      }

      @keyframes finger_fadein {
         0% {
            opacity: 0;
            right: -100px;
            bottom: 600px
         }

         100% {
            opacity: 1;
            right: 40px;
            bottom: 180px
         }
      }

      @keyframes finger_fadeout {
         0% {
            opacity: 1;
            right: 40px;
            bottom: 180px
         }

         100% {
            opacity: 0;
            right: 280px;
            bottom: 180px
         }
      }
   </style>
